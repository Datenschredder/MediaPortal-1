      <texture mask="video_poster_mask.png">blue.png</texture>
      <colordiffuse>70ffffff</colordiffuse>
    </control>
    <control>
      <description>buttons</description>
      <type>group</type>
      <animation effect="slide" time="70" start="-300,0">WindowOpen</animation>
