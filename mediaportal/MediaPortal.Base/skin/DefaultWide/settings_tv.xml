        <onup>17</onup>
      </control>
      <control>
        <description>H.264 Video Codecs</description>
        <type>button</type>
        <id>35</id>
        <label>6036</label>
        <width>350</width>
      </control>
      <control>
        <description>Audio Codecs</description>
        <type>button</type>
        <id>25</id>
        <label>6001</label>
        <width>350</width>
      </control>
      <control>
        <description>LATM AAC Audio Codecs</description>
        <type>button</type>
        <id>36</id>
        <label>6039</label>
        <width>350</width>
      </control>
      <control>
        <description>Audio Renderer</description>
        <type>button</type>
        <id>33</id>
        <label>6002</label>
        <width>350</width>
      </control>
      <control>
        <description>De-interlacing</description>
        <type>button</type>
        <id>27</id>
        <label>6003</label>
        <width>350</width>
      </control>
      <control>
        <description>Apect Ratio</description>
        <type>button</type>
        <id>28</id>
        <label>6004</label>
        <width>350</width>
        <ondown>30</ondown>
